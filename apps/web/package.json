    "@mantine/hooks": "^7.12.1",
    "@mantine/modals": "^7.12.1",
    "@mantine/notifications": "^7.12.1",
    "backend-api-sdk": "workspace:*",
    "framer-motion": "^11.2.6",
    "jwt-decode": "^4.0.0",
    "mantine-form-zod-resolver": "^1.1.0",
    "next": "14.2.5",
