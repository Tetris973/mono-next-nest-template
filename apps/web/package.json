    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.1.0",
    "pino-pretty": "^11.2.1",
    "postcss": "^8.4.41",
    "postcss-preset-mantine": "^1.17.0",
