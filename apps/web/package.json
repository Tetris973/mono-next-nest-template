{
  "name": "web",
  "version": "0.1.0",
  "files": [
    ".next/standalone/apps/web/"
  ],
  "private": true,
  "scripts": {
    "env:dev": "export NODE_ENV=development && dotenv -e .env.development.local",
    "env:prod": "export NODE_ENV=production && dotenv -e .env.production.local",
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "test:e2e": "cd ../server && pnpm db-seed:force:prod && cd ../web && pnpm env:prod -- playwright test",
    "test:e2e:ui": "pnpm test:e2e --ui",
    "lint": "next lint --fix",
    "ts-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "debug": "NODE_OPTIONS='--inspect' next dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
